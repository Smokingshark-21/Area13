<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke
            android:width="1dp"
                android:color="@color/md_theme_dark_primary" /> <!-- Hier können Sie die gewünschte Stroke-Farbe setzen -->
            <corners android:radius="10dp" /> <!-- Hier können Sie den gewünschten Radius setzen -->
        </shape>
    </item>
</layer-list>